package com.webservice.server;

import javax.jws.WebMethod;
import javax.jws.WebService;
/**
 * jdk方式开发webservice
 * @author moon
 *
 */
@WebService
public class Test {
	
	@WebMethod
     public int squared(int x) {
    	int result = x*x;
    	System.out.println("求平方"+x+"-----"+result);
    	return result;
     }
}
